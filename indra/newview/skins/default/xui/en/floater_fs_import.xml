<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater
  name="fs_import"
  help_topic="fs_import"
  title="Object Import"
  width="210"
  height="216"
  save_rect="true"
  can_resize="false"
  can_minimize="true"
  can_close="true">
    <floater.string
     name="file_status">
        [LINKSETS] linksets. [PRIMS] prims. [TEXTURES] textures.
        [SOUNDS] sounds. [ANIMATIONS] animations.
    </floater.string>
    <floater.string
     name="file_status_error">
        Error reading file. Select another file.
    </floater.string>
    <floater.string
     name="file_version_error">
        Error parsing file, wrong format or version.
    </floater.string>
    <floater.string
     name="file_status_running">
        Linkset [LINKSET] of [LINKSETS], prim [PRIM] of [PRIMS]
    </floater.string>
    <floater.string
     name="file_status_done">
        Finished importing.
    </floater.string>
     <floater.string
     name="texture_uploading">
        Uploading texture [TEXTURE] of [TEXTURETOTAL].
    </floater.string>
    <floater.string
     name="sound_uploading">
        Uploading sound [SOUND] of [SOUNDTOTAL].
    </floater.string>
    <floater.string
     name="animation_uploading">
        Uploading animation [ANIMATION] of [ANIMATIONTOTAL].
    </floater.string>
    <floater.string
     name="asset_uploading">
        Uploading asset [ASSET] of [ASSETTOTAL].
    </floater.string>
    <floater.string
     name="upload_cost">
        Total cost of uploads: $L [COST].
    </floater.string>
    <line_editor
      name="filename"
      bevel_style="in"
      border_style="line"
      border_thickness="1"
      enabled="false"
      height="20"
      width="200"
      left="5"
      follows="left|top"
      max_length_chars="256" />
    <text
     type="string"
     length="1"
     follows="left|top"
     height="40"
     layout="topleft"
     left="5"
     name="file_status_text"
     width="200">
        Select a file.
    </text>
    <button
      name="pick_file"
      label="Choose File"
      height="20"
      width="97"
      left="5"
      follows="left|top">
	<button.commit_callback
	    function="Import.PickFile" />
    </button>
    <button
      name="import_file"
      label="Import"
      height="20"
      width="97"
      left_delta="103"
      enabled="false"
      follows="left|top" >
	<button.commit_callback
	    function="Import.ImportLinkset" />
    </button>
	<text
	 type="string"
	 visible="false"
	 length="1"
	 follows="left|top"
	 height="14"
	 layout="topleft"
	 left="5"
	 name="creation_date_text"
	 width="200">
		File created on [DATE_STRING]
    </text>
	<text
	 type="string"
	 visible="false"
	 length="1"
	 follows="left|top"
	 height="14"
	 layout="topleft"
	 left="5"
	 name="author_text"
	 use_ellipses="true"
	 width="200">
		by [AUTHOR] [GRID]
    </text>
	<text
	 type="string"
	 visible="false"
	 length="1"
	 follows="left|top"
	 height="14"
	 layout="topleft"
	 left="5"
	 name="client_text"
	 use_ellipses="true"
	 width="200">
		Client: [CLIENT] v.[VERSION]
    </text>
    <check_box
      follows="top|left"
      height="16"
      layout="topleft"
      left="5"
      enabled="false"
      name="do_not_attach"
      label="Do not attach object"
      top_pad="10"/>
    <check_box
      follows="top|left"
      height="16"
      layout="topleft"
      left="5"
      enabled="false"
      name="region_position"
      label="Restore Region Position"
      top_pad="5"/>
    <check_box
      follows="top|left"
      height="16"
      layout="topleft"
      left="5"
      width="90"
      enabled="false"
      name="upload_asset"
      label="Upload content"
      top_pad="5" >
	<check_box.commit_callback
	    function="Import.UploadAsset" />
    </check_box>
    <check_box
      follows="top|left"
      height="16"
      layout="topleft"
      enabled="false"
      visible="false"
      name="temp_asset"
      label="temp" >
      <check_box.commit_callback
	    function="Import.TempAsset" />
    </check_box>
</floater>
