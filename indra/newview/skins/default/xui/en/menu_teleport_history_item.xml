<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<context_menu
 layout="topleft"
 name="Teleport History Item Context Menu">
    <menu_item_call
     label="Teleport"
     layout="topleft"
     name="Teleport">
        <menu_item_call.on_click
         function="TeleportHistory.Action"
         parameter="teleport" />
    </menu_item_call>
    <menu_item_call
     label="View"
     layout="topleft"
     name="More Information">
        <menu_item_call.on_click
         function="TeleportHistory.Action"
         parameter="view" />
    </menu_item_call>
    <menu_item_call
     label="Show on map"
     layout="topleft"
     name="show_on_map">
        <menu_item_call.on_click
         function="TeleportHistory.Action"
         parameter="show_on_map" />
    </menu_item_call>
    <menu_item_call
     label="Copy SLurl"
     layout="topleft"
     name="CopyToClipboard">
        <menu_item_call.on_click
         function="TeleportHistory.Action"
         parameter="copy_slurl" />
    </menu_item_call>
</context_menu>
